# Supabase Configuration
# Development
NEXT_PUBLIC_SUPABASE_URL_DEV=http://localhost:8000
NEXT_PUBLIC_SUPABASE_ANON_KEY_DEV=your-anon-key
SUPABASE_SERVICE_ROLE_KEY_DEV=your-service-role-key

# Production
NEXT_PUBLIC_SUPABASE_URL_PROD=your-production-supabase-url
NEXT_PUBLIC_SUPABASE_ANON_KEY_PROD=your-production-anon-key
SUPABASE_SERVICE_ROLE_KEY_PROD=your-production-service-role-key

# Database Configuration
DATABASE_URL_DEV=postgresql://postgres:your-super-secret-password@localhost:5432/postgres
DATABASE_URL_PROD=your-production-database-url

# S3 Configuration
S3_BUCKET_NAME_DEV=your-dev-bucket-name
S3_BUCKET_NAME_PROD=your-production-bucket-name
AWS_ACCESS_KEY_ID_DEV=your-aws-access-key
AWS_ACCESS_KEY_ID_PROD=your-production-aws-access-key
AWS_SECRET_ACCESS_KEY_DEV=your-aws-secret-key
AWS_SECRET_ACCESS_KEY_PROD=your-production-aws-secret-key
AWS_REGION_DEV=your-aws-region
AWS_REGION_PROD=your-production-aws-region

# API Configuration
API_KEY_SECRET_DEV=your-api-key-secret
API_KEY_SECRET_PROD=your-production-api-key-secret
RATE_LIMIT_REQUESTS_DEV=100
RATE_LIMIT_REQUESTS_PROD=1000
RATE_LIMIT_WINDOW_MS_DEV=900000  # 15 minutes in milliseconds
RATE_LIMIT_WINDOW_MS_PROD=900000  # 15 minutes in milliseconds

# File Upload Limits
MAX_FILE_SIZE_MB_DEV=5
MAX_FILE_SIZE_MB_PROD=10
ALLOWED_FILE_TYPES_DEV=".pdf,.doc,.docx,.txt,.jpg,.jpeg,.png"
ALLOWED_FILE_TYPES_PROD=".pdf,.doc,.docx,.txt,.jpg,.jpeg,.png"

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key

# Langsmith Configuration
# Langsmith Configuration
LANGSMITH_TRACING_V2=true
LANGSMITH_ENDPOINT="https://api.smith.langchain.com"
LANGSMITH_API_KEY=your-langsmith-api-key
LANGSMITH_PROJECT="zenzen"